@model IEnumerable<MVCLabTaskLayout.Models.Student>

@{
    ViewBag.Title = "_StudentTablePartial";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>_StudentTablePartial</h2>


@foreach (var item in Model)
{
    <tr>
        <td>@item.StudentId</td>
        <td>@item.Name</td>
        <td>@item.EmailAddress</td>
        <td>@item.Mobile</td>
        <td>@item.Gender</td>
        <td>@item.DateOfRegistration.ToString("dd-MM-yyyy HH:mm:ss")</td>
        <td>@item.CourseId</td>
        <td>@item.Course?.Name</td>
        <td>@item.Course?.Fees</td>
        <td>@item.Course?.Duration</td>
        <td>@item.Address</td>
        <td>@item.FeeStatus</td>
        <td>
            <!-- Actions -->
        </td>
    </tr>
}



